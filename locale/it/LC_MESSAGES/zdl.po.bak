#: ../ui/ui.sh:533
#, sh-format
msgid "${BGreen}   d ${Color_Off}│ start ${BGreen}d${Color_Off}aemon"
msgstr "${BGreen}   d ${Color_Off}│ avvia ${BGreen}d${Color_Off}emone"

#: ../ui/ui.sh:535
#, sh-format
msgid ""
"${BGreen}   c ${Color_Off}│ ${BGreen}c${Color_Off}lean temporary files of "
"completed downloads\n"
"     │\n"
"${BRed}   K ${Color_Off}│ stop all downloads and every instance of ZDL in "
"the directory (${BRed}K${Color_Off}ill-all)"
msgstr ""
"${BGreen}   c ${Color_Off}│ ${BGreen}c${Color_Off}ancella i file temporanei "
"dei download completati\n"
"     │\n"
"${BRed}   K ${Color_Off}│ interrompi tutti i download e ogni istanza di ZDL "
"nella directory (${BRed}K${Color_Off}ill-all)"

#: ../ui/ui.sh:540
#, sh-format
msgid ""
"${BRed}   Q ${Color_Off}│ stop an active instance of $PROG in $PWD but not "
"the downloads already started"
msgstr ""
"${BRed}   Q ${Color_Off}│ ferma un'istanza attiva di $PROG in $PWD lasciando "
"attivi i downloader già avviati"

#: ../ui/ui.sh:542
#, sh-format
msgid ""
"     │\\n${BBlue}   q ${Color_Off}│ ${BBlue}q${Color_Off}uit from $PROG --"
"interactive "
msgstr ""
"     │\\n${BBlue}   q ${Color_Off}│ esci da $PROG --interactive (${BBlue}"
